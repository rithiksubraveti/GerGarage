@model GerGarage.Models.JobDetail

@{
    ViewBag.Title = "Invoice";
}

<h1>Ger' Garage</h1>


<div>

    <hr />
    <dl class="dl-horizontal">

        <dt>
            <b> Customer Name:</b>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.CustomerName)
        </dd>

        <dt>
            <b> Booking Id:</b>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.BookingId)
        </dd>

        <dt>
