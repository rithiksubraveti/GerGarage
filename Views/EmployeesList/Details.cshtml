@model GerGarage.Models.EmployeeRegistry

@{
    ViewBag.Title = "Details";
}


<div>
    <h4>Employee Details</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <b>First Name</b>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EmployeeFirstName)
        </dd>

        <dt>
            <b>Last Name</b>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EmployeeLastName)
        </dd>

        <dt>
            <b>Contact Number</b>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EmployeeContact)
        </dd>

        <dt>
            <b>Email Id</b>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EmployeeEmailId)
        </dd>

        <dt>
            <b>Password</b>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EmployeePassword)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
