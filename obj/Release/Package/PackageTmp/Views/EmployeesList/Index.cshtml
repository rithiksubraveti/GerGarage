@model IEnumerable<GerGarage.Models.EmployeeRegistry>

@{
    ViewBag.Title = "Index";
}

<h2>Employees List</h2>
<p>@Html.ActionLink("Create", "Create")</p>

<table class="table">
    <tr>
        <th>
            First Name

        </th>
        <th>
            Last Name

        </th>
        <th>
            Contact Number

        </th>
        <th>
            Email Id

        </th>
        @*<th>
                Password
                @Html.DisplayNameFor(model => model.EmployeePassword)
            </th>*@
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeFirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeLastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeContact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployeeEmailId)
            </td>
            @* <td>
                    @Html.DisplayFor(modelItem => item.EmployeePassword)
                </td>*@
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
